{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "react-imported-component/babel",
    "@babel/plugin-proposal-dynamic-import",
    "@babel/plugin-transform-modules-commonjs"
  ],
  "env": {
    "server": {
    },
    "client": {
      "plugins": [
        ["@babel/plugin-transform-runtime", {
          "regenerator": true
        }],
        "react-hot-loader/babel"
      ]
    }
  }
}