{
  "name": "git-client",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "parcel app/index.html",
    "build": "npm run generate-imported-component && rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env BABEL_ENV=client parcel build --no-source-maps -d dist/client --public-url /assets app/index.html",
    "build:server": "cross-env BABEL_ENV=server parcel build --no-source-maps -d dist/server --target node server/index.js",
    "generate-imported-component": "imported-components app app/imported.js",
    "start": "node dist/server",
    "lint": "stylint src",
    "plop": "plop"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@sifrr/server": "^0.0.6",
    "cross-env": "^6.0.3",
    "parcel-bundler": "^1.12.3",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-hot-loader": "^4.12.14",
    "react-imported-component": "^6.2.0",
    "rimraf": "^3.0.0",
    "stylus": "^0.54.7",
    "stylus-bem-sugar": "^1.0.4",
    "uWebSockets.js": "uNetworking/uWebSockets.js#v15.11.0",
    "wouter": "^2.2.1"
  },
  "devDependencies": {
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "plop": "^2.4.0",
    "stylint": "^2.0.0"
  }
}
